{  
  "name": "MyTaskList",  
  "version": "2",
  "manifest_version":2,  
  "description": "Management my everyday's task lists.",  
  "browser_action": {  
    "default_icon": "icon.png" ,
    "default_title": "My Task List",
    "default_popup": "popup.html"
  } ,
    "content_scripts" : [
    {
      "matches" : [ "http://*/*" ],
      "js" : [ "contentscript.js" ],
      "run_at" : "document_idle",
      "all_frames" : false
    }
  ],
  "permissions": [
  "http://iciba.com/",
  "https://www.google.com/"
]

} 